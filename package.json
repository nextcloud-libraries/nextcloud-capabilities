	],
	"engines": {
		"node": "^14.0.0",
		"npm": "^7.0.0"
	},
	"dependencies": {
		"@babel/cli": "^7.18.10",
		"@babel/core": "^7.18.10",
		"@babel/preset-env": "^7.18.10",
		"@babel/preset-typescript": "^7.18.6",
		"@nextcloud/browserslist-config": "^2.2.0",
